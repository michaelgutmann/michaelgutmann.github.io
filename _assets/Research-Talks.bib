@Article{Gutmann-2017-05-09,
  journal   = {Workshop in Inverse Problems and Data Science, University of Edinburgh},
  title     = {Bayesian Inference by Density Ratio Estimation},
  year      = {2017},
  month     = {May},
  abstract  = {This talk is about Bayesian inference when the likelihood function cannot be computed but data can be generated from the model. The model's data generating process is allowed to be arbitrarily complex. Exact solutions are then not possible. But by re-formulating the original problem as a problem of estimating the ratio between two probability density functions, I show how e.g. logistic regression can be used to obtain approximate solutions. The proposed inference framework is illustrated on stochastic nonlinear dynamical models.},
  doi       = {https://github.com/michaelgutmann/inverse-problems-pres},
  file      = {Gutmann-2017-05-09.pdf:Gutmann-2017-05-09.pdf:PDF},
  location  = {Edinburgh, UK},
  owner     = {mgutmann},
  pdf       = {Gutmann-2017-05-09.pdf},
  source    = {/home/mgutmann/3-Archive/Presentations/2017-05-LMS-WS-Inverse-Problems/git-repo/inverse-problems-pres},
  timestamp = {2017.09.18},
  url       = {https://www.turing.ac.uk/events/inverse-problems-data-science/},
