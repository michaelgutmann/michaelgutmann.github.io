@Article{Gutmann-2017-12-13,
  journal   = {Helsinki AI Day, Helsinki, Finland},
  title     = {Machine Learning for Complex Data Analysis},
  year      = {2017},
  month     = {December},
  abstract  = {In science and engineering, we are often confronted with the analysis of complex and noisy data. Statistical modelling and inference provide fundamental principles for data analysis. But for complex models and data sources, the computational cost of implementing the principles and performing inference exactly is prohibitively large. I present some of my work on approximate yet computationally feasible inference and explain how machine learning helped us to reduce the computational cost by factors of 1000 or more.},
