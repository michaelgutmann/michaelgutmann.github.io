@Article{Gutmann-2016-04-21,
  journal   = {CRiSM Workshop: Estimating Constants, Department of Statistics, University of Warwick, UK},
  title     = {Noise-Contrastive Estimation and its Generalizations},
  year      = {2016},
  month     = {April},
  abstract  = {Parametric statistical models are often not properly normalized, that is, they do not integrate to unity. While unnormalized models can, in principle, be normalized by dividing them by their integral, the cost of computing the integral is generally prohibitively large. This is an issue because without normalization, the likelihood function is not available for performing inference. I present a method called "noise-contrastive estimation" where unnormalized models are estimated by solving a classification problem. I explain some of its properties and applications, and show that it is part of a general estimation framework based on the SBregman divergence.},
  file      = {Gutmann-2016-04-21.pdf:Gutmann-2016-04-21.pdf:PDF},
  location  = {Department of Statistics, University of Warwick, Coventry, UK},
  owner     = {mgutmann},
  pdf       = {Gutmann-2016-04-21.pdf},
  source    = {/home/mgutmann/3-Archive/Presentations/2016-04-Warwick-Workshop/Presentation},
  timestamp = {2017.09.23},
  url       = {https://www2.warwick.ac.uk/fac/sci/statistics/crism/workshops/estimatingconstants/},
  venue     = {CRiSM Workshop: Estimating Constants},
  website   = {1},
}

@Article{Gutmann-2016-05-17,
  journal   = {Approximate Bayesian Computation in Helsinki, Helsinki, Finland},
  title     = {Fast Likelihood-Free Inference via Bayesian Optimization},
  year      = {2016},
  month     = {May},
  abstract  = {Statistical models may be specified in terms of stochastic computer program - a simulator - which can generate samples from the model for any configuration of the parameters. While such models support complex data generating mechanisms, the likelihood function is generally incomputable which renders statistical inference difficult. Several likelihoodfree inference methods have been proposed which share the basic idea of identifying the model parameters by finding values for which the discrepancy between simulated and observed data is small. Examples are indirect inference and approximate Bayesian computation. A major obstacle to using these methods is their computational cost. The cost is largely due to the need to repeatedly simulate data sets and the lack of knowledge about how the parameters affect the discrepancy. We propose a strategy which combines probabilistic modeling of the discrepancy with optimization to facilitate likelihood-free inference. The strategy is implemented using Bayesian optimization and is shown to accelerate the inference through a reduction in the number of required simulations by several orders of magnitude.},
  file      = {Gutmann-2016-05-17.pdf:Gutmann-2016-05-17.pdf:PDF},
  location  = {Helsinki, Finland},
  owner     = {mgutmann},
  pdf       = {Gutmann-2016-05-17.pdf},
  source    = {/home/mgutmann/3-Archive/Presentations/2016-05-ABCruise/ABC-Tutorial},
  timestamp = {2017.09.22},
  url       = {http://www.hiit.fi/ABCruise/programme.php},
  venue     = {ABCruise},
  website   = {1},
}

